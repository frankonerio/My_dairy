{
  "name": "my_dairy",
  "version": "1.0.0",
  "description": "server",
  "main": "server.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node API/server.js",
    "start": "node build/server.js",
    "build": "npm run clean  && babel API --out-dir build",
    "clean": "rm -rf build",
    "db-test-init": "npx sequelize-cli db:migrate:undo:all --env test --debug && npx sequelize-cli db:migrate --env test --debug",
    "test": "npm run db-test-init && npx cross-env NODE_ENV=test mocha --require @babel/register tests/**.test.js --timeout 20000 --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankonerio/My_dairy.git"
  },
  "keywords": [
    "node",
    "server"
  ],
  "author": "MIT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frankonerio/My_dairy/issues"
  },
  "homepage": "https://github.com/frankonerio/My_dairy#readme",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}
